Include conf.d/*.conf

Match exec "~/Workspace/garbetjie/dotfiles/ssh/is_workspace.sh tencent"
    IdentityFile ~/.ssh/keys/tencent
    IdentitiesOnly yes

Match exec "~/Workspace/garbetjie/dotfiles/ssh/is_workspace.sh"
    IdentityFile ~/.ssh/keys/personal
    IdentitiesOnly yes

Host *
    AddKeysToAgent yes
    Compression yes
    ServerAliveInterval 30s
    UseKeychain yes
